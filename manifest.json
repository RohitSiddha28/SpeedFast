{
  "manifest_version": 3,
  "name": "YouTube Ultra Speed",
  "version": "1.1",
  "description": "2.5x and 3x playback speeds for YouTube",
  "permissions": ["scripting"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "css": ["content.css"],
      "run_at": "document_idle"
    }
  ]
}
